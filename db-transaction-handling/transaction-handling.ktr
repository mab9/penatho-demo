    <single_statement>Y</single_statement>
    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>
UPDATE mfh_sandbox_deckung
SET
    grp_selbstbehalt_1d = selbstbehalt_1d.gruppe,
    grp_selbstbehalt_md = selbstbehalt_md.gruppe

FROM mfh_sandbox_deckung deckung
    JOIN mfh_sandbox_objekt objekt on deckung.objekt_id = objekt.id
    JOIN mfh_sandbox_police police on police.id = objekt.police_id

-- Selbstbehalt 1-Dimensional
LEFT JOIN mfh_grp_selbstbehalt selbstbehalt_1d ON
    selbstbehalt_1d.eindimensional = true AND
    ((selbstbehalt_1d.wertebereich_min &lt;= deckung.selbstbehalt AND selbstbehalt_1d.wertebereich_max >= deckung.selbstbehalt)
        OR (selbstbehalt_1d.wertebereich_min is null AND deckung.selbstbehalt is null))

-- Selbstbehalt Mehr-Dimensional
LEFT JOIN mfh_grp_selbstbehalt selbstbehalt_md ON
    selbstbehalt_md.eindimensional = false AND
    ((selbstbehalt_md.wertebereich_min &lt;= deckung.selbstbehalt AND selbstbehalt_md.wertebereich_max >= deckung.selbstbehalt)
        OR (selbstbehalt_md.wertebereich_min is null AND deckung.selbstbehalt is null))

-- END OF deckung UPDATE
WHERE mfh_sandbox_deckung.id = deckung.id
    AND police.statistikjahr = ?::integer
    AND mfh_sandbox_deckung.grp_selbstbehalt_1d IS NULL
;

</sql>
